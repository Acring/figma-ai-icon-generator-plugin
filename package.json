{
  "name": "figma-ai-icon-generator",
  "version": "1.0.0",
  "description": "Figma 插件：使用 AI 生成风格统一的图标和插图",
  "license": "MIT",
  "scripts": {
    "dev": "webpack --mode=development --watch",
    "build": "webpack --mode=production",
    "build:watch": "webpack --mode=development --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "prettier:format": "prettier --write './src/**/*.{js,jsx,ts,tsx,css,json}'",
    "prepare": "husky"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@figma/plugin-typings": "^1.50.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "postcss": "^8.4.49",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.4.2",
    "react-dev-utils": "^12.0.1",
    "style-loader": "^3.3.1",
    "tailwindcss": "^4.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{css,json}": [
      "prettier --write"
    ]
  }
}
